<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jeeranut.testsqlite">

    <uses-permission android:name="android.permission.SEND_SMS"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ResultActivity"
            android:theme="@style/AppTheme"
            android:label="@string/app_name"
            android:launchMode="singleTask">
        </activity>
        <activity
            android:name="AuditeeActivity"
            android:theme="@style/AppTheme"
            android:label="Auditee">
        </activity>
        <activity
            android:name=".AuditeeWipActivity"
            android:theme="@style/AppTheme"
            android:label="Auditee WIP"
            android:configChanges="keyboardHidden|orientation|screenSize">
        </activity>
        <activity
            android:name=".AuditeeRmActivity"
            android:theme="@style/AppTheme"
            android:label="Auditee RM"
            android:configChanges="keyboardHidden|orientation|screenSize">
        </activity>
        <activity
            android:name=".AuditorActivity"
            android:theme="@style/AppTheme"
            android:label="Auditor"
            android:configChanges="keyboardHidden|orientation|screenSize">
        </activity>
        <activity
            android:name=".AuditorWipActivity"
            android:theme="@style/AppTheme"
            android:label="Auditor WIP"
            android:configChanges="keyboardHidden|orientation|screenSize">
        </activity>
        <activity
            android:name=".AuditorRmActivity"
            android:theme="@style/AppTheme"
            android:label="Auditor RM"
            android:configChanges="keyboardHidden|orientation|screenSize">
        </activity>
        <activity
            android:name=".AuditeeWipDeleteActivity"
            android:theme="@style/AppTheme"
            android:label="Auditee Wip Delete"
            android:configChanges="keyboardHidden|orientation|screenSize">
        </activity>
        <activity
            android:name=".AuditeeRmDeleteActivity"
            android:theme="@style/AppTheme"
            android:label="Auditee Rm Delete"
            android:configChanges="keyboardHidden|orientation|screenSize">
        </activity>
        <activity
            android:name=".AuditeeWipEditActivity"
            android:theme="@style/AppTheme"
            android:label="Auditee Wip Edit"
            android:configChanges="keyboardHidden|orientation|screenSize">
        </activity>
        <activity
            android:name=".AuditeeRmEditActivity"
            android:theme="@style/AppTheme"
            android:label="Auditee Rm Edit"
            android:configChanges="keyboardHidden|orientation|screenSize">
        </activity>

    </application>

</manifest>